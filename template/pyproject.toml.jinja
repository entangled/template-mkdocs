[tool.poetry]
name = "{{project_name}}"
version = "0.1.0"
description = "Virtual env for running entangled and building docs."
authors = ["{{project_author}}"]
# You may still want to add a license here
# license = "Apache 2"
readme = "README.md"
packages = []

[tool.poetry.dependencies]
python = ">=3.11,<3.13"
entangled-cli = "^2.0.0"
mkdocs-entangled-plugin = "^0.4.0"
mkdocs = "^1.5.2"
mkdocs-material = "^9.2.6"

[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"
